{
    "compilerOptions": {
        "target": "ES2016",
        "module": "CommonJS",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "strict": true,
    },
    "exclude": [
        "node_modules",
        "dist"
    ]
}